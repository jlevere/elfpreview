{
  "name": "elfpreview",
  "displayName": "elfPreview",
  "description": "Preview ELF binary files in VS Code",
  "author": "https://github.com/jlevere",
  "version": "0.0.1",
  "keywords": [
    "VSCode",
    "Elf",
    "Editor",
    "Preview"
  ],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jlevere/elfpreview.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/jlevere/elfpreview/issues"
  },
  "homepage": "https://github.com/jlevere/elfpreview#readme",
  "engines": {
    "vscode": "^1.98.0",
    "node": "^v22.13.1"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "elfpreview.showPreview",
        "title": "ELF: Show Preview"
      }
    ],
    "customEditors": [
      {
        "viewType": "elfpreview.elfViewer",
        "displayName": "ELF Preview",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ],
        "priority": "option"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "elfpreview.showPreview",
          "when": "resourceScheme == 'file'"
        }
      ],
      "explorer/context": [
        {
          "command": "elfpreview.showPreview",
          "when": "resourceScheme == 'file'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "pnpm lint && pnpm esbuild && pnpm run compile:rust",
    "package": "pnpm lint && pnpm node esbuild.js --production",
    "vscode:prepublish": "pnpm package",
    "esbuild": "node esbuild.js",
    "compile:rust": "cd rust && cargo build --release --target wasm32-unknown-unknown && cp ./target/wasm32-unknown-unknown/release/*.wasm ../dist && cd ..",
    "compile:ts": "tsc -b tsconfig.json",
    "watch:ts": "tsc -b tsconfig.json -w",
    "watch:esbulid": "pnpm node esbulid.js --watch",
    "clean": "rimraf ./dist",
    "lint": "eslint ./ts/src ./webview/src",
    "compile-tests": "tsc -p .",
    "pretest": "pnpm compile-tests",
    "test": "vscode-test",
    "generate:wit": "wit2ts --outDir ./ts/src ./wit/elf-parser.wit"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.23.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "~20.17.25",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/wasm-component-model": "^1.0.1",
    "esbuild": "^0.25.1",
    "esbuild-svelte": "^0.9.2",
    "eslint": "^9.23.0",
    "eslint-plugin-svelte": "^3.3.3",
    "globals": "^16.0.0",
    "rimraf": "^6.0.1",
    "svelte": "^5.25.3",
    "svelte-check": "^4.1.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.28.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}